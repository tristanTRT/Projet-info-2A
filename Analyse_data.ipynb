    "# Nouvelle session si la pause d√©passe le seuil\n",
    "df_all[\"new_session\"] = (\n",
    "    (df_all[\"delta_minutes\"] > SESSION_GAP) | (df_all[\"delta_minutes\"].isna())\n",
    ")\n",
    "\n",
    "# Identifiant de session\n",
    "df_all[\"session_id\"] = df_all.groupby(\"user_id\")[\"new_session\"].cumsum()\n"
