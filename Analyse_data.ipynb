    "\n",
    "def max_win_streak(series):\n",
    "    if len(series) == 0:\n",
    "        return 0\n",
    "    change = series.ne(series.shift())\n",
    "    groups = change.cumsum()\n",
    "    return series.groupby(groups).sum().max()\n",
    "\n",
    "# Joueurs avec suffisamment de parties\n",
    "valid_users = df_reg[\"user_id\"].value_counts()\n",
    "valid_users = valid_users[valid_users > 20].index\n",
    "\n",
    "df_sim = df_reg[df_reg[\"user_id\"].isin(valid_users)].copy()\n",
    "\n",
    "real_mean_streak = (\n",
    "    df_sim.groupby(\"user_id\")[\"win\"]\n",
    "          .apply(max_win_streak)\n",
    "          .mean()\n",
    ")\n",
    "\n",
    "# Simulation\n",
    "n_sim = 500\n",
    "simulated_means = []\n",
    "\n",
    "for _ in range(n_sim):\n",
    "    shuffled = (\n",
    "        df_sim.groupby(\"user_id\")[\"win\"]\n",
    "              .transform(np.random.permutation)\n",
    "    )\n",
    "    sim_mean = shuffled.groupby(df_sim[\"user_id\"]).apply(max_win_streak).mean()\n",
    "    simulated_means.append(sim_mean)\n",
    "\n",
